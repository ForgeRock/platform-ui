{
  "name": "@forgerock/platfrom-enduser",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "unit": "jest --no-cache --silent",
    "unit:debug": "node --inspect-brk node_modules/.bin/vue-cli-service test:unit --no-cache --watch --runInBand",
    "snapshot": "jest --updateSnapshot",
    "coverage": "jest --verbose --coverage --noStackTrace --no-cache --silent"
  },
  "dependencies": {
    "appauthhelper": "0.2.0",
    "axios": "0.19.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "bootstrap": "4.3.1",
    "bootstrap-vue": "2.0.0-rc.22",
    "core-js": "3.1.4",
    "es6-promise": "4.2.4",
    "lodash": "4.17.15",
    "moment": "2.22.2",
    "oidcsessioncheck": "0.0.4",
    "vee-validate": "2.0.8",
    "vue": "2.6.10",
    "vue-i18n": "8.0.0",
    "vue-js-toggle-button": "1.2.3",
    "vue-notification": "1.3.16",
    "vue-router": "3.0.1",
    "vue-spinner": "1.0.3",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@babel/polyfill": "7.4.4",
    "@fortawesome/fontawesome-free": "5.9.0",
    "@vue/cli-plugin-babel": "3.8.0",
    "@vue/cli-plugin-eslint": "3.8.0",
    "@vue/cli-plugin-unit-jest": "3.11.0",
    "@vue/cli-service": "3.8.0",
    "@vue/eslint-config-standard": "4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.9.0",
    "cross-env": "5.0.1",
    "eslint": "6.5.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-vue": "5.2.3",
    "git-revision-webpack-plugin": "3.0.3",
    "jest": "24.9.0",
    "jest-serializer-vue": "2.0.2",
    "moxios": "0.4.0 ",
    "mutationobserver-shim": "0.3.3",
    "node-sass": "4.12.0",
    "popper.js": "1.15.0",
    "portal-vue": "2.1.4",
    "sass-loader": "7.1.0",
    "style-resources-loader": "1.2.1",
    "vue-cli-plugin-i18n": "0.6.0",
    "vue-jest": "3.0.5",
    "vue-template-compiler": "2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "import/extensions": [
        "error",
        "always",
        {
          "vue": "never",
          "js": "never"
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "indent": [
        "error",
        2
      ],
      "arrow-parens": [2, "always"],
      "no-tabs": 0,
      "max-len": 0,
      "vue/html-closing-bracket-newline": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}