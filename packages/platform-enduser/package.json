    "axios": "1.6.1",
    "bootstrap": "4.6.1",
    "bootstrap-vue": "2.22.0",
    "brace": "0.11.1",
    "core-js": "3.32.0",
    "d3-simple-slider": "1.10.4",
    "dayjs": "1.11.7",
    "es6-promise": "4.2.4",
    "lodash": "4.17.21",
    "oidcsessioncheck-enduser": "npm:oidcsessioncheck@1.1.0",
