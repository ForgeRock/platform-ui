    "oidcsessioncheck": "0.0.4",
    "vee-validate": "2.0.8",
    "vue": "2.6.10",
    "vue-i18n": "8.0.0",
    "vue-js-toggle-button": "1.2.3",
    "vue-notification": "1.3.16",
    "vue-router": "3.0.1",
    "vue-spinner": "1.0.3"
  },
  "devDependencies": {
    "@babel/polyfill": "7.4.4",
    "@fortawesome/fontawesome-free": "5.9.0",
    "@vue/cli-plugin-babel": "3.8.0",
    "@vue/cli-plugin-eslint": "3.8.0",
