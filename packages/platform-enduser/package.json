    "dev": "vue-cli-service serve",
    "docker:build": "config/scripts/docker:build.sh",
    "docker:run": "config/scripts/docker:run.sh",
    "lint": "vue-cli-service lint",
    "test:unit:watch": "vue-cli-service test:unit --watch",
