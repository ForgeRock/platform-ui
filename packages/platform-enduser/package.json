    "vue": "2.7.14",
    "vue-i18n": "8.17.0",
    "vue-notification": "1.3.16",
    "vue-router": "3.0.1",
    "vue-sanitize": "0.2.2",
    "vue-spinner": "1.0.3",
    "vuex": "3.0.1",
    "whatwg-fetch": "3.2.0"
