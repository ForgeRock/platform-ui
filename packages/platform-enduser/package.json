{
  "name": "@forgerock/platform-enduser",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "yarn lint:js",
    "build": "vue-cli-service build",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "vue-cli-service lint",
    "lint:style": "vue-cli-service lint:style '**/*.vue' '**/*.scss'",
    "dev": "vue-cli-service serve",
    "unit": "jest --no-cache",
    "unit:debug": "node --inspect-brk node_modules/.bin/jest --no-cache --watch --runInBand",
    "snapshot": "jest --updateSnapshot",
    "coverage": "jest --verbose --coverage --noStackTrace --no-cache --silent",
    "test:e2e": "../../e2e/passwords.sh cypress run",
    "find-copyright-problems": "echo 'Listing all files which do not contain a valid copywright header for this year - some files do not qualify \n' && cd src && git grep -L --word-regexp \"Copyright (c) $(date +\"%Y\")\""
  },
  "dependencies": {
    "@forgerock/platform-shared": "1.0.0",
    "appauthhelper": "0.3.17",
    "axios": "0.19.0",
    "bootstrap": "4.3.1",
    "bootstrap-vue": "2.13.0",
    "core-js": "3.1.4",
    "dayjs": "1.8.20",
    "es6-promise": "4.2.4",
    "lodash": "4.17.19",
    "oidcsessioncheck": "1.0.1",
    "ua-parser-js": "0.7.21",
    "vee-validate": "3.2.2",
    "vue": "2.6.11",
    "vue-i18n": "8.0.0",
    "vue-js-toggle-button": "1.2.3",
    "vue-notification": "1.3.16",
    "vue-router": "3.0.1",
    "vue-spinner": "1.0.3",
    "vuex": "3.0.1",
    "whatwg-fetch": "3.2.0"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "1.1.2",
    "@vue/cli-plugin-babel": "3.8.0",
    "@vue/cli-plugin-eslint": "4.3.1",
    "@vue/cli-service": "4.3.1",
    "@vue/eslint-config-airbnb": "4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "chai": "4.1.2",
    "copy-webpack-plugin": "5.0.3",
    "cypress": "5.0.0",
    "eslint": "5.16.0",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-vue": "5.0.0",
    "flush-promises": "1.0.2",
    "jest": "26.4.2",
    "jest-html-reporters": "1.2.0",
    "jest-serializer-vue": "2.0.2",
    "lint-staged": "8.1.5",
    "node-sass": "4.12.0",
    "sass-loader": "8.0.2",
    "style-resources-loader": "1.2.1",
    "vue-cli-plugin-i18n": "0.6.0",
    "vue-jest": "3.0.5",
    "vue-template-compiler": "2.6.11"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
