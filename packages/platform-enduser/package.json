    "jest-html-reporters": "1.2.0",
    "lint-staged": "8.1.5",
    "sass": "1.52.1",
    "sass-loader": "8.0.2",
    "style-resources-loader": "1.2.1",
    "vue-cli-plugin-i18n": "2.3.2",
    "vue-template-compiler": "2.7.14"
  },
