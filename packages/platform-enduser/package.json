  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "import/extensions": [
        "error",
        "always",
        {
          "vue": "never",
          "js": "never"
        }
      ],
      "no-param-reassign": ["error", { "props": false }],
      "indent": [2, "tab"],
      "no-tabs": 0,
      "max-len": 0,
      "vue/html-closing-bracket-newline": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
