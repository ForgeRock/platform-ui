{
  "pluginsFile": "e2e/plugins/index.js",
  "chromeWebSecurity": false,
  "retries": 3,
  "reporter": "../../node_modules/mochawesome",
  "reporterOptions": {
    "overwrite": false,
    "html": false,
    "json": true
  },
  "ignoreTestFiles": ["lighthouse.suite.js"],
  "env": {
    "lighthousePassThreshold": {
      "performance": 0.75,
      "accessibility": 0.75,
      "best-practices": 0.75
    }
  }
}
