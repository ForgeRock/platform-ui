{
  "name": "@forgerock/platform-login",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "dev": "vue-cli-service serve",
    "docker:build": "config/scripts/docker:build.sh",
    "docker:run": "config/scripts/docker:run.sh",
    "lint": "vue-cli-service lint",
    "unit": "jest --no-cache",
    "coverage": "jest --verbose --coverage --noStackTrace --no-cache"
  },
  "dependencies": {
    "@forgerock/platform-components": "1.0.0",
    "bootstrap": "4.3.1",
    "bootstrap-vue": "2.0.0-rc.27",
    "core-js": "3.1.4",
    "forgerockembeddedlogin": "0.0.3",
    "lodash": "4.17.15",
    "regenerator-runtime": "0.13.3",
    "vee-validate": "2.0.8",
    "vue": "2.6.10",
    "vue-i18n": "8.0.0",
    "vue-js-toggle-button": "1.2.3",
    "vue-notification": "1.3.16",
    "vue-router": "3.0.7",
    "vue-spinner": "1.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.8.0",
    "@vue/cli-plugin-eslint": "3.8.0",
    "@vue/cli-plugin-unit-mocha": "3.8.0",
    "@vue/cli-service": "3.8.0",
    "@vue/eslint-config-airbnb": "4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.9.0",
    "chai": "4.1.2",
    "cross-env": "5.0.1",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "5.0.0",
    "jest": "24.9.0",
    "jest-serializer-vue": "2.0.2",
    "lint-staged": "8.1.5",
    "node-sass": "4.12.0",
    "popper.js": "1.15.0",
    "sass-loader": "7.1.0",
    "sinon": "7.3.2",
    "style-resources-loader": "1.2.1",
    "vue-cli-plugin-i18n": "0.6.0",
    "vue-jest": "3.0.5",
    "vue-template-compiler": "2.6.10"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "import/extensions": [
        "error",
        "always",
        {
          "vue": "never",
          "js": "never"
        }
      ],
      "no-param-reassign": ["error", { "props": false }],
      "indent": [
        "error",
        2
      ],
      "no-tabs": 0,
      "max-len": 0,
      "vue/html-closing-bracket-newline": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
