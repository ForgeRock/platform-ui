{
  "name": "forgerock-platform-ui",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "unit": "jest",
    "unit:watch": "jest --watch",
    "postinstall": "husky install",
    "test:cb": "jest --config=jest.config.crossbrowser.js",
    "test:docker:build": "docker build -t platform-ui-e2e-testrunner -f docker-e2e/Dockerfile .",
    "test:docker:run": "docker run -it --network host -v /var/run/docker.sock:/var/run/docker.sock -v ~/.ssh:/root/.ssh -v ~/.config/gcloud:/root/.config/gcloud -e DEPLOYMENT_NAMESPACE=$USER platform-ui-e2e-testrunner",
    "coverage-diff": "yarn g:coverage-diff"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.22.9",
    "@testing-library/dom": "7.30.0",
