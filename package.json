    "jest-watch-typeahead": "0.6.1",
    "mocha": "8.1.1",
    "mochawesome": "6.1.1",
    "mochawesome-merge": "4.1.0",
    "nano-staged": "0.8.0",
    "nock": "13.0.4",
    "sass": "1.52.1",
    "postcss-html": "1.5.0",
    "sass-loader": "8.0.2",
    "selenium-webdriver": "4.0.0-alpha.7",
    "style-resources-loader": "1.2.1",
